let isLoggedIn = "Hemant";
let BooleanIsLoggedIn = Boolean(isLoggedIn);

// console.log(BooleanIsLoggedIn);
// console.log(typeof BooleanIsLoggedIn);

// 1 => true; 0 => false
// "" => false
// "Hemant" => true

